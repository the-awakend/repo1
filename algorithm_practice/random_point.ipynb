    "import numpy as np \n",
    "def sphereNormalPoint():\n",
    "    for i in range(100):\n",
    "        x = np.random.normal(0,1)\n",
    "        y = np.random.normal(0,1)\n",
    "        z = np.random.normal(0,1)\n",
    "        r = (x*x + y*y + z*z) ** (1/2)\n",
    "        print('x = ', x/r, 'y = ', y/r, 'z = ', z/r)\n",
    "\n",
    "sphereNormalPoint()"
   ]
